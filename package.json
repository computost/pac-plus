{
  "name": "pac-plus",
  "version": "0.0.5",
  "description": "An opinionated collection of ALM-related tasks for Power Platform.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prebuild": "npm run set-version",
    "build": "tsc",
    "prestart": "npm run set-version",
    "start": "node --loader ts-node/esm src/cli.ts",
    "set-version": "node --loader ts-node/esm setVersion.ts",
    "test": "node --loader ts-node/esm src/test.ts"
  },
  "bin": {
    "pac-plus": "dist/cli.js"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "pac",
    "Power Platform",
    "Microsoft",
    "ALM",
    "DevOps"
  ],
  "author": "Melody Universe",
  "license": "ISC",
  "dependencies": {
    "commander": "^8.2.0",
    "execa": "^5.1.1",
    "odata-query": "^6.7.1",
    "pac-wrap": "0.0.31",
    "xrm-webapi-node": "0.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/xml2js": "^0.4.9",
    "dotenv": "^10.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
